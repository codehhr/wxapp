}

/* title */
.title {
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  background-color: #fdebc3;
  box-shadow: 0 2px 4px #ffa551;
  text-align: center;
  font-size: 2rem;
  font-weight: 800;
  color: #e97d4c;
}